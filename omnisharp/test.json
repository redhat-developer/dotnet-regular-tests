{
  "name": "omnisharp",
  "enabled": true,
  "version": "3.0",
  "versionSpecific": false,
  "type": "bash",
  "cleanup": true,
  "timeoutMultiplier": 2,
  "skipWhen": [
    "version=7", // see https://github.com/redhat-developer/dotnet-regular-tests/issues/212
    "version=8", // see https://github.com/redhat-developer/dotnet-regular-tests/issues/250
    "version=9"  // see https://github.com/redhat-developer/dotnet-regular-tests/issues/250
  ],
  "ignoredRIDs":[
    "linux-musl",
    "linux-s390x",
    "linux-ppc64le"
  ]
}
